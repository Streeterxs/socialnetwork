    "typescript": "^3.7.5"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind:css": "tailwind build src/tailwind.css -c tailwind.config.js -o src/index.css",
    "start": "npm run relay && npm run tailwind:css && react-scripts start",
